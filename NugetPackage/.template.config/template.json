{
    "$schema": "http://json.schemastore.org/template",
    "author": "Chriss Barnard",
    "classifications": [
        "Common",
        "Code"
    ],
    "identity": "netmaximum.nuget.template",
    "name": "Netmaximum nuget template",
    "shortName": "nm-nuget",
    "tags": {
        "language": "C#",
        "type": "item"
    },
    "symbols": {
        "ProjectName": {
            "type": "parameter",
            "replaces": "PROJECT_NAME",
            "FileRename": "PROJECT_NAME",
            "isRequired": true
        }
    },
    "sources": [
        {
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                ".template.config/**/*",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json",
                ".vs/**"
            ]
        }
    ]
}